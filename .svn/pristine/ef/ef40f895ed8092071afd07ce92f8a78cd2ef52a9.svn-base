<getRWart>
<![CDATA[SELECT 
  B.MATNR,
  D.location_name,
  E.aisle_lst,
  A.location_id,
  list(COALESCE(A.WEIGHT, A.LTO_QTY)||' '||CASE WHEN A.MEINH = C.MEINS AND B.RANDOM_WEIGHT_FLG  = 'Y' THEN B.PI_UOM ELSE A.MEINH END CASE,' ') LTO_QTY,
  CASE WHEN D.location_type ='I' THEN 'Active' ELSE 'Not Active' END CASE AS Active_Indicator
    FROM saap.local_wow_lto_details  A
    JOIN saap.saap.local_wow_lto I ON (A.matnr=I.matnr)
    JOIN saap.zwowc_article_listing B ON (A.matnr=B.matnr)
    JOIN saap.zwowc_article_uom C ON (A.matnr=C.matnr and A.meinh=C.meinh)
    JOIN  saap.zwowt_C_lto_location D ON( A.location_id=D.location_id)
    JOIN (SELECT list(aisle) AS aisle_lst,location_id FROM saap.zwowt_c_lto_aisle GROUP BY location_id) E ON (A.location_id=E.location_id)
    JOIN saap.zwowc_article_listing F ON (A.matnr=F.matnr)
   WHERE F.random_weight_flg='Y'
    GROUP BY  
    B.MATNR, B.MATNR, 
    D.location_name,
    E.aisle_lst,
    A.location_id,
    Active_Indicator]]>
</getRWart>
