<![CDATA[select b.MATNR,b.PLVKP ,b.meinh from saap.ZWOWC_ARTICLE_LISTING a, saap.ZWOWC_PROMO_ITM b , saap.ZWOWC_ARTICLE_UOM d , saap.ZWOWC_ARTICLE_MASTER e 
where a.matnr=b.matnr and b.matnr=d.matnr and d.MATNR=e.matnr and e.PBD_IND='Y' and d.BASE_UOM_IND='Y' and a.ALC_STATUS in ('NA','RA','DA') and a.ALC_STATUS<>'ES' and 
b.aktnr in (select aktnr from saap.zwowc_promo_hdr where today() between VKDAB and VKDBI) group by b.matnr,b.PLVKP,b.meinh having count(b.matnr)=1]]>
</ActivePBDArt>
<ActivePBDgArt>
<![CDATA[select b.MATNR,b.PLVKP,b.meinh from saap.ZWOWC_ARTICLE_EAN a, saap.ZWOWC_PROMO_ITM b , saap.ZWOWC_ARTICLE_LISTING c
where a.EAN11=b.matnr and c.alc_status<>'ES' and a.EANTP='Z2' and 
b.aktnr in (select aktnr from saap.zwowc_promo_hdr where today() between VKDAB and VKDBI) group by b.matnr,b.PLVKP,b.meinh]]>
</ActivePBDgArt>
<ActiveCPBDArt>
<![CDATA[select b.MATNR,b.PLVKP ,b.meinh from saap.ZWOWC_ARTICLE_LISTING a, saap.ZWOWC_PROMO_ITM b , saap.ZWOWC_ARTICLE_UOM d 
where a.matnr=b.matnr and b.matnr=d.matnr and d.BASE_UOM_IND='Y' and a.ALC_STATUS in ('NA','RA','DA') and a.ALC_STATUS<>'ES' and a.COMPLEX_PACK_BRK_ARTICLE>1 and a.COMPLEX_PACK_BRK_OM>6 and 
