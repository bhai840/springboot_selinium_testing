<getCUParticlePBD>
<![CDATA[SELECT SUB.MATNR, SUB.PBD_IND, SUB.ARTICLE_CAT_ID, SUB.STD_CUP_PRICE
FROM (
    SELECT ZAM.MATNR, ZAM.PBD_IND, ZAM.ARTICLE_CAT_ID, 
    fn_wow_get_std_cup_price(ZAM.MATNR) AS STD_CUP_PRICE
    FROM saap.ZWOWC_ARTICLE_MASTER ZAM
    JOIN saap.ZWOWC_ARTICLE_UOM ZAU ON (ZAM.MATNR = ZAU.MATNR)
    JOIN ZWOWC_ARTICLE_LISTING ZA ON (ZAM.MATNR = ZA.MATNR)
    WHERE ZAU.BASE_UOM_IND = 'Y'
    AND ZAM.PBD_IND='Y'
    AND ZAM.MASTER_SIZE > 0
    AND ZAM.COMP_SIZE_AU > 0
    AND ZAM.COMP_SIZE_AU_UOM <> ''
    AND ZAM.MASTER_SIZE_UOM <> ''
    AND EXISTS (
        SELECT 1
        FROM saap.MAT_VIEW_ZWOW_ARTICLE_SELL_PRICE SP
        WHERE SP.MATNR=ZAM.MATNR
        AND SP.MEINH=ZAU.MEINH AND ZA.ALC_STATUS<>'ES'
    )
) SUB
WHERE SUB.STD_CUP_PRICE > 0;]]>
</getCUParticlePBD>
<getCUParticleCPBD>
<![CDATA[SELECT SUB.MATNR, SUB.STD_CUP_PRICE, SUB.PBD_IND, SUB.ARTICLE_CAT_ID 
FROM ( SELECT top 50 ZAM.MATNR, ZAM.PBD_IND, ZAM.ARTICLE_CAT_ID, fn_wow_get_std_cup_price(ZAM.MATNR) AS STD_CUP_PRICE 
FROM saap.ZWOWC_ARTICLE_MASTER ZAM JOIN saap.ZWOWC_ARTICLE_UOM ZAU ON (ZAM.MATNR = ZAU.MATNR) 
join saap.zwowc_article_listing zal on (zal.matnr=zam.matnr and is_cpbd='1')
WHERE ZAU.BASE_UOM_IND = 'Y' AND ZAM.MASTER_SIZE > 0 AND ZAM.COMP_SIZE_AU > 0 AND ZAM.COMP_SIZE_AU_UOM <> '' AND ZAM.MASTER_SIZE_UOM <> ''
AND EXISTS ( SELECT 1 FROM saap.MAT_VIEW_ZWOW_ARTICLE_SELL_PRICE SP WHERE SP.MATNR=ZAM.MATNR AND SP.MEINH=ZAU.MEINH AND zal.ALC_STATUS<>'ES'))SUB 
WHERE SUB.STD_CUP_PRICE > 0 ;]]>
</getCUParticleCPBD>
</TCName>

